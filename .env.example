# Server Configuration
PORT=3000
NODE_ENV=development

# Admin Authentication
ADMIN_PASSWORD=your_admin_password_here
ADMIN_SECRET=your_admin_secret_here

# GitHub Configuration (for file storage and uploads)
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repo_name

# Telegram Bot Token (same value used for bot and API authentication)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Payment API
API_BANK=https://your-bank-api-url-here

# External Services
APPTESTER_URL=https://repository.apptesters.org/

# Telegram Bot Admin IDs (comma-separated)
TELEGRAM_ADMIN_IDS=your_telegram_user_id

# API URL (for bot to communicate with web server)
API_URL=https://cheatlibrary.fly.dev